{
  "name": "nat-checker",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Client-side NAT type detection tool using WebRTC STUN protocol",
  "scripts": {
    "dev": "tsx server-custom.ts",
    "build": "pnpm build:server && next build",
    "build:server": "tsc server/stun-server.ts --outDir dist --module esnext --moduleResolution bundler --esModuleInterop --skipLibCheck --target es2022",
    "start": "NODE_ENV=production tsx server-custom.ts",
    "dev:nextonly": "next dev",
    "dev:stun": "tsx server/stun-server.ts",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "lint": "biome check",
    "format": "biome format --write"
  },
  "dependencies": {
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.544.0",
    "motion": "^12.23.24",
    "next": "16.0.5",
    "next-intl": "^4.5.6",
    "next-themes": "^0.4.6",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "schema-dts": "^1.1.5",
    "tailwind-merge": "^3.3.1",
    "tsx": "^4.19.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "concurrently": "^9.1.2",
    "shadcn": "^3.4.2",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad",
  "pnpm": {
    "overrides": {
      "@types/react": "19.2.7",
      "@types/react-dom": "19.2.3"
    }
  }
}
